{
  "name": "Catan Card Counter",
  "description": "Counts Resource Cards for colonist.io",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "icon64.png"
    }
  }
}